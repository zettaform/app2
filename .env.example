# AWS Configuration
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_DEFAULT_REGION=us-east-1

# Application Settings
NODE_ENV=production
PORT=3001
ENVIRONMENT=prod

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-in-production

# CORS Configuration
CORS_ORIGIN=http://localhost:5174,http://localhost:3000,https://your-domain.com

# DynamoDB Tables
DDB_USERS_TABLE=production-Users
DDB_CUSTOMERS_TABLE=production-Customers
DDB_FEEDBACK_TABLE=production-Feedback
DDB_ORDERS_TABLE=production-Orders
DDB_ANALYTICS_TABLE=production-Analytics
DDB_ADMIN_KEYS_TABLE=admin-keys-table-admin-keys
DDB_EXTERNAL_LOGS_TABLE=production-external-user-creation-logs

# S3 Configuration
S3_AVATARS_BUCKET=your-avatars-bucket-name

# Request Limits
MAX_REQUEST_SIZE=10mb